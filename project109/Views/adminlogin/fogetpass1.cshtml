@model project109.Models.UsersAdmins
@{
    ViewBag.Title = "fogetpass1";
    Layout = "~/Views/Shared/_LayoutNoTemp.cshtml";
}

<script src="~/Scripts/SpryAssets/SpryValidationTextField.js" type="text/javascript"></script>
<link href="~/Scripts/SpryAssets/SpryValidationTextField.css" rel="stylesheet" type="text/css" />
<h2>Enter Your Email</h2>
<form action="fogetpass1" method="post">
    <table width="200" border="0" align="center" cellpadding="0" cellspacing="5">
        <tr>
           
            <td nowrap="nowrap">
                <span id="sprytextfield2">
                    @if (Request["s"] == "no")
                    {@Html.Raw("Email not found") }
                    <span class="textfieldRequiredMsg">Enter Email</span><span class="textfieldInvalidFormatMsg">Not Correct</span><input type="text" name="txtmailchk" id="txtmailchk" />
                </span>
            </td>
            <td align="left"><input type="submit" name="button2" id="button2" value="Go" /></td>
        </tr>
    </table>
</form>
<script type="text/javascript">

var sprytextfield2 = new Spry.Widget.ValidationTextField("sprytextfield2", "email", {validateOn:["blur"]});
</script>

